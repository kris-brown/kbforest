\quiver{\begin{tikzcd}
    E & {E'} & E & {E'} \\
    V & {V'} & V & {V'}
    \arrow["{f_E}", from=1-1, to=1-2]
    \arrow["src"', from=1-1, to=2-1]
    \arrow["{src'}", from=1-2, to=2-2]
    \arrow["{f_E}", from=1-3, to=1-4]
    \arrow["tgt"', from=1-3, to=2-3]
    \arrow["{tgt'}", from=1-4, to=2-4]
    \arrow["{f_V}"', from=2-1, to=2-2]
    \arrow["{f_V}"', from=2-3, to=2-4]
  \end{tikzcd}}

