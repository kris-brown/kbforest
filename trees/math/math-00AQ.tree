\title{Homomorphism of internal groups}
\import{macros}
\import{rel}

\example{math-00AV}


\p{Given two [internal groups](math-00AM) in some category #{\cat{C}} with products, #{(M,\cdot_M,e_M,(-)_M^{-1})} and #{(N,\cdot_N,e_N,(-)_N^{-1})}, a \em{morphism of internal groups} is a morphism #{G\rightarrow H} that commutes with the interpretations of the operations, i.e.:}

% https://q.uiver.app/#

\quiver{
  \begin{tikzcd}
    {M^2} & {N^2} & M & N & 1 & 1 \\
    M & N & M & N & M & N
    \arrow["{h^2}", from=1-1, to=1-2]
    \arrow["{\cdot_M}"', from=1-1, to=2-1]
    \arrow["{\cdot_N}", from=1-2, to=2-2]
    \arrow["h", from=1-3, to=1-4]
    \arrow["{(-)^{-1}_M}"', from=1-3, to=2-3]
    \arrow["{(-)^{-1}_N}", from=1-4, to=2-4]
    \arrow[equals, from=1-5, to=1-6]
    \arrow["{e_M}"', from=1-5, to=2-5]
    \arrow["{e_N}", from=1-6, to=2-6]
    \arrow["h"', from=2-1, to=2-2]
    \arrow["h"', from=2-3, to=2-4]
    \arrow["h"', from=2-5, to=2-6]
  \end{tikzcd}
}

\examples